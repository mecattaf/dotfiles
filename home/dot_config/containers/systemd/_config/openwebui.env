---

# OpenWebUI Environment Configuration
# Complete setup with all auxiliary services

# Core Settings
PORT=8080
WEBUI_URL=http://localhost:3000
WEBUI_NAME=AI Assistant
ENV=prod
WEBUI_AUTH=true
ENABLE_SIGNUP=false
DEFAULT_USER_ROLE=user
WEBUI_SECRET_KEY=change-this-secret-key-to-something-secure
ANONYMIZED_TELEMETRY=false

# Database Configuration
DATABASE_URL=postgresql://llm_admin:llm-postgres-password@llm-postgres/openwebui
PGVECTOR_DB_URL=postgresql://llm_admin:llm-postgres-password@llm-postgres/openwebui
VECTOR_DB=pgvector

# Redis Configuration
REDIS_URL=redis://llm-redis:6379/2
REDIS_KEY_PREFIX=open-webui
WEBSOCKET_MANAGER=redis
WEBSOCKET_REDIS_URL=redis://llm-redis:6379/3

# LLM Configuration (via LiteLLM)
OPENAI_API_BASE_URL=http://litellm:4000/v1
OPENAI_API_KEY=sk-llm-master-key-change-this
ENABLE_OLLAMA_API=false
ENABLE_LITELLM_API=false
ENABLE_OPENAI_API=true

# Document Processing
CONTENT_EXTRACTION_ENGINE=tika
TIKA_SERVER_URL=http://tika:9998
DOCLING_SERVER_URL=http://docling:5001
PDF_EXTRACT_IMAGES=true

# Web Search
ENABLE_RAG_WEB_SEARCH=true
ENABLE_WEB_SEARCH=true
RAG_WEB_SEARCH_ENGINE=searxng
WEB_SEARCH_ENGINE=searxng
SEARXNG_QUERY_URL=http://searxng:8080/search?q=<query>
RAG_WEB_SEARCH_CONCURRENT_REQUESTS=10
RAG_WEB_SEARCH_RESULT_COUNT=6

# Tool Server (MCP)
TOOL_SERVER_CONNECTIONS=[{"url":"http://mcp:8000/time","path":"openapi.json","auth_type":"none","key":"","config":{"enable":true}},{"url":"http://mcp:8000/postgres","path":"openapi.json","auth_type":"none","key":"","config":{"enable":true}},{"url":"http://mcp:8000/filesystem","path":"openapi.json","auth_type":"none","key":"","config":{"enable":true}}]

# Features
ENABLE_EVALUATION_ARENA_MODELS=false
ENABLE_IMAGE_GENERATION=false
ENABLE_COMMUNITY_SHARING=false
ENABLE_WEBSOCKET_SUPPORT=true

# Performance
HF_HUB_OFFLINE=1
RAG_TEXT_SPLITTER=token
RAG_EMBEDDING_ENGINE=openai
RAG_EMBEDDING_MODEL=text-embedding-3-small
RAG_OPENAI_API_BASE_URL=http://litellm:4000/v1
RAG_OPENAI_API_KEY=sk-llm-master-key-change-this

# Logging
GLOBAL_LOG_LEVEL=info
